{
    "name": "learninghouse/phinx",
    "type": "library",
    "description": "Phinx makes it ridiculousy easy to manage the database migrations for your PHP app.",
    "keywords": ["phinx", "migrations", "database", "db", "database migrations"],
    "homepage": "http://phinx.org",
    "license": "MIT",
    "authors": [{
        "name": "Rob Morgan",
        "email": "robbym@gmail.com",
        "homepage": "http://robmorgan.id.au"    
    }],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "2.2.1",
        "symfony/config": "2.2.1",
        "symfony/class-loader": "2.2.1",
        "symfony/yaml": "2.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Phinx": "src/"
